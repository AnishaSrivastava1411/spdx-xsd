   <xs:element name="Checksum" abstract="true">
      <xs:annotation>
         <xs:documentation>A Checksum is value that allows the contents of a file to be authenticated. Even small changes to the content of the file will change its checksum. This class
            allows the results of a variety of checksum and cryptographic message digest algorithms to be represented. </xs:documentation>
      </xs:annotation>
   </xs:element>
   <xs:complexType name="ChecksumClassType">
      <xs:annotation>
         <xs:documentation>A Checksum is value that allows the contents of a file to be authenticated. Even small changes to the content of the file will change its checksum. This class allows the results of a variety of checksum and cryptographic message digest algorithms to be represented.</xs:documentation>
         <xs:appinfo>
            <rdfs:subClassOf>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://spdx.org/rdf/terms#checksumValue"/>
                  <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                  <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#hexBinary"/>
               </owl:Restriction>
            </rdfs:subClassOf>
            <rdfs:subClassOf>
               <xs:element name="" abstract="true">
                  <xs:annotation>
                     <xs:documentation/>
                  </xs:annotation>
               </xs:element>
               <xs:complexType name="ClassType">
                  <xs:annotation>
                     <xs:documentation/>
                     <xs:appinfo>
                        <owl:unionOf rdf:parseType="Collection">
                           <owl:Restriction>
                              <owl:onProperty rdf:resource="http://spdx.org/rdf/terms#algorithm"/>
                              <owl:hasValue rdf:resource="http://spdx.org/rdf/terms#checksumAlgorithm_md5"/>
                           </owl:Restriction>
                           <owl:Restriction>
                              <owl:onProperty rdf:resource="http://spdx.org/rdf/terms#algorithm"/>
                              <owl:hasValue rdf:resource="http://spdx.org/rdf/terms#checksumAlgorithm_sha1"/>
                           </owl:Restriction>
                           <owl:Restriction>
                              <owl:onProperty rdf:resource="http://spdx.org/rdf/terms#algorithm"/>
                              <owl:hasValue rdf:resource="http://spdx.org/rdf/terms#checksumAlgorithm_sha256"/>
                           </owl:Restriction>
                        </owl:unionOf>
                     </xs:appinfo>
                  </xs:annotation>
               </xs:complexType>
            </rdfs:subClassOf>
            <ns:term_status xml:lang="en">stable</ns:term_status>
         </xs:appinfo>
      </xs:annotation>
   </xs:complexType>
   <xs:complexType name="ConjunctiveLicenseSetType">
      <xs:annotation>
         <xs:documentation>A ConjunctiveLicenseSet represents a set of licensing information all of which apply.</xs:documentation>
         <xs:appinfo>
            <rdfs:subClassOf rdf:resource="http://spdx.org/rdf/terms#AnyLicenseInfo"/>
            <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Container"/>
            <rdfs:subClassOf>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://spdx.org/rdf/terms#member"/>
                  <owl:onClass rdf:resource="http://spdx.org/rdf/terms#AnyLicenseInfo"/>
                  <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
               </owl:Restriction>
            </rdfs:subClassOf>
            <ns:term_status xml:lang="en">stable</ns:term_status>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="Type">
            <xs:element ref=""/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
