               </owl:Restriction>
            </rdfs:subClassOf>
            <rdfs:subClassOf>
               <xs:element name="" abstract="true">
                  <xs:annotation>
                     <xs:documentation/>
                  </xs:annotation>
               </xs:element>
               <xs:complexType name="ClassType">
                  <xs:annotation>
                     <xs:documentation/>
