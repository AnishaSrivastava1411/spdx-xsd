   <xs:complexType name="FileType">
      <xs:annotation>
         <xs:documentation>A File represents a named sequence of information that is contained in a software package.</xs:documentation>
         <xs:appinfo>
            <rdfs:subClassOf rdf:resource="http://spdx.org/rdf/terms#SpdxItem"/>
            <rdfs:subClassOf>
