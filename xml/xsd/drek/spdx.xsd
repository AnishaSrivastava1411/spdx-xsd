                  <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
               </owl:Restriction>
            </rdfs:subClassOf>
            <ns:term_status xml:lang="en">stable</ns:term_status>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="Type">
            <xs:element ref=""/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="DisjunctiveLicenseSetClassType">
      <xs:annotation>
         <xs:documentation>A DisjunctiveLicenseSet represents a set of licensing information where only one license applies at a time. This class implies that the recipient gets to choose one of these licenses they would prefer to use.</xs:documentation>
         <xs:appinfo>
            <rdfs:subClassOf rdf:resource="http://spdx.org/rdf/terms#AnyLicenseInfo"/>
