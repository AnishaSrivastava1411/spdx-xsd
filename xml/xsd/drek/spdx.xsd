                  <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
               </owl:Restriction>
            </rdfs:subClassOf>
            <ns:term_status>stable</ns:term_status>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="Type">
            <xs:element ref=""/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
