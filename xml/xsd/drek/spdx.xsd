   <xs:complexType name="DisjunctiveLicenseSetType">
      <xs:annotation>
         <xs:documentation>A DisjunctiveLicenseSet represents a set of licensing information where only one license applies at a time. This class implies that the recipient gets to choose one of these licenses they would prefer to use.</xs:documentation>
         <xs:appinfo>
            <rdfs:subClassOf rdf:resource="http://spdx.org/rdf/terms#AnyLicenseInfo"/>
            <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Container"/>
