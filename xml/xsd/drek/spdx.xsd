                  <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
               </owl:Restriction>
            </rdfs:subClassOf>
            <rdfs:subClassOf>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://spdx.org/rdf/terms#created"/>
