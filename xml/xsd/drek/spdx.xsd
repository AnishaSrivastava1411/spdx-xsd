   <xs:element name="CreationInfo" abstract="true">
      <xs:annotation>
         <xs:documentation>A CreationInfo provides information about the individuals, organizations and tools involved in the creation of an SpdxDocument.</xs:documentation>
      </xs:annotation>
   </xs:element>
   <xs:complexType name="CreationInfoClassType">
      <xs:annotation>
