   <xs:element name="ExternalDocumentRef" abstract="true">
      <xs:annotation>
         <xs:documentation>Information about an external SPDX document reference including the checksum. This allows for verification of the external references.</xs:documentation>
      </xs:annotation>
   </xs:element>
   <xs:complexType name="ExternalDocumentRefClassType">
      <xs:annotation>
         <xs:documentation>Information about an external SPDX document reference including the checksum. This allows for verification of the external references.</xs:documentation>
         <xs:appinfo>
            <rdfs:subClassOf>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://spdx.org/rdf/terms#checksum"/>
                  <owl:onClass rdf:resource="http://spdx.org/rdf/terms#Checksum"/>
                  <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
               </owl:Restriction>
            </rdfs:subClassOf>
            <rdfs:subClassOf>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://spdx.org/rdf/terms#externalDocumentId"/>
                  <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                  <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
               </owl:Restriction>
            </rdfs:subClassOf>
            <rdfs:subClassOf>
